{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "redirects": [
      {
        "source": "/project/(.*)",
        "destination": "/",
        "statusCode": 301
      },
      {
        "source": "/(.*)",
        "destination": "/",
        "statusCode": 301
      }
    ],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
  }